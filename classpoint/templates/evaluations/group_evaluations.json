{% spaceless %}
{
  "draw": {{draw|escapejs}},
  "recordsTotal": {{total_records|escapejs}},
  "recordsFiltered": {{total_display_records|escapejs}},
  "data":[
    {% for group_evaluation in queryset %}
    [
      "{{ group_evaluation.evaluation.activity.activity_id }}",
      "{{ group_evaluation.evaluation.activity.learning_target.name }}",
      "{{ group_evaluation.evaluation.activity.learning_target.get_level_display }}",
      "{{ group_evaluation.group }}",
      "{{ group_evaluation.get_status_display }}",
      "<p style=\"margin-top:12px\" class=\"text text-{% if not group_evaluation.sent %}{{ group_evaluation.evaluation.status_class }}{% else %}success{% endif %}\">{{ group_evaluation.evaluation.due_date|date:'d/m/Y' }}</p>",
      "{% if not group_evaluation.sent %}<a class='btn btn-info' href='{% url 'evaluations:evaluate' group_evaluation.pk %}'>Evaluar</a>{% else %}<a class='btn btn-success' href='{% url 'evaluations:view' group_evaluation.pk %}'>Resultados</a> <a style='margin-left:10px' class='btn btn-danger btn-delete' href='{% url 'evaluations:group_evaluation_delete' group_evaluation.pk %}'>Eliminar</a> <a style='margin-left:10px' class='btn btn-warning' href='download-pdf/?group_evaluation_id={{group_evaluation.pk}}&activity_id={{ group_evaluation.evaluation.activity.activity_id }}&learning_target_name={{ group_evaluation.evaluation.activity.learning_target.name }}&level={{ group_evaluation.evaluation.activity.learning_target.get_level_display }}&group={{ group_evaluation.group }}&status={{ group_evaluation.get_status_display }}&due_date={{ group_evaluation.evaluation.due_date|date:'d/m/Y' }}'>Imprimir PDF</a>{% if group_evaluation %}<h2 data-title=\"Remedial - {{ group_evaluation.evaluation.activity.activity_id }}\" data-url=\"{% url 'learning_targets:target_remedial_ajax' group_evaluation.evaluation.activity.learning_target.id %}\" class=\"remedial-item\" style=\"display:inline; margin-left:10px;cursor:pointer\"><span style=\"margin-left:0px;\" class=\"badge badge-warning text-danger\">R</span></h2>{% endif %}{% endif %} {% if group_evaluation.draft %}<a style='margin-left:10px' class='btn btn-warning btn-finalize' href='{% url 'evaluations:finalize' group_evaluation.pk %}'>Finalizar</a><a style='margin-left:10px' class='btn btn-danger btn-delete' href='{% url 'evaluations:group_evaluation_delete' group_evaluation.pk %}'>Eliminar</a>{% endif %}"
    ]{% if not forloop.last %},{% endif %}
    {% endfor %}
  ]
}
{% endspaceless %}
